@model IEnumerable<StudentInfo.Domain.Model.Student>

<div class="row">
    @Html.ActionLink("Add new student", "Add")
</div>
<table>
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.FirstName</td>
                <td>@student.LastName</td>
            </tr>
        }
    </tbody>
</table>